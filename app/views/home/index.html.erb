<% if current_user.has_role('Admin') %>
  <%= render 'dashboards/payment_dashboard' %>
<% elsif @institution %>
    <%= render 'dashboards/institution' %>
<% elsif current_user.has_role('Member') and current_user.member.try(:status)==true %>
    <%= render 'dashboards/member' %>
<% elsif current_user.has_role('Member') and (current_user.member.blank? or current_user.member.try(:status).blank?)   %>
    <%= render 'dashboards/guest' %>
<% elsif current_user.has_role(User::CPDPROVIDER) %>
    <%= render 'dashboards/cpd_provider' %>
<% end %>