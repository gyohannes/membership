<%= simple_form_for(@association_detail) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :short_name %>
    <%= f.input :contact_phone %>
    <%= f.input :contact_email %>
    <%= f.input :address %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
