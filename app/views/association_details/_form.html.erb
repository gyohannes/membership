<%= simple_form_for(@association_detail) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :short_name %>
    <%= f.input :logo, as: :file %>
    <%= f.input :contact_phone %>
    <%= f.input :contact_email %>
    <%= f.input :address %>
    <%= f.input :about_association,label: 'About', placeholder: 'About Association', input_html: {class: 'wysihtml5'} %>
  </div>

  <div class="form-actions">
    <%= f.button :submit, class: 'btn btn-primary' %>
  </div>
<% end %>
