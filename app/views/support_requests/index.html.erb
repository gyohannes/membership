<div class="panel panel-default">
  <div class="panel-heading">New Support Requests</div>
  <div class="panel-body">
    <table class="table table-hover dataTable js-exportable">
      <thead>
      <th></th>
      <th></th>
      </thead>
      <tbody>
      <% @support_requests.each do |support_request| %>
          <tr class="unread">
            <td><%= link_to "#{support_request.sender_user}", support_request %></td>
            <td><%= support_request.subject %></td>
          </tr>
      <% end %>
      </tbody>
    </table>
  </div>
</div>